<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.quandoo.tablebooking">

    <application
        android:name=".application.QuandooApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.CustomerActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.TableReservationActivity"
            android:label="@string/title_activity_table_reservation"
            android:theme="@style/AppTheme"></activity>

        <receiver android:name=".receiver.TableClearAlarmReciever"></receiver>

    </application>

</manifest>